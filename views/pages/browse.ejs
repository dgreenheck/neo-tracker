<!DOCTYPE html>
<html>
<head>
  <title>Browse NEOs</title>
  <%- include('../partials/style-headers.ejs') %>
</head>
<body>
  <h1>Near Earth Objects</h1>
  <%- include('../partials/browse-table.ejs') %>
    <div class="pagination-control">
      <% for(var i = Math.max(page.number - 5, 0); i <= Math.min(page.number + 5, page.total_pages); i++) { %>
        <a class="pagination-link<%= i == page.number ? " selected" : "" %>" href="/browse/<%= i %>"">
          <%= i %>
        </a>
        <% } %>
    </div>
    <hr>
    <%- include('../partials/stats.ejs') %>
</body>

</html>